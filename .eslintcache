 className=\"mb-3\">\n                    <DropdownButton\n                        as={InputGroup.Prepend}\n                        variant=\"outline-secondary\"\n                        title={CAT_TO_TITLE[currentCategory]}\n                        id=\"input-group-dropdown-1\"\n                        onSelect={onCategorySelected}\n                    >\n                        {\n                            CATEGORIES.map(cat => <Dropdown.Item key={cat} eventKey={cat}>{CAT_TO_TITLE[cat]}</Dropdown.Item>)\n                        }\n                    </DropdownButton>\n                    <Form.Control\n                        ref={searchEl} value={search} onChange={onSearchInput} type=\"text\" placeholder=\"The Walking Dead, Johnny Depp...\"\n                    />\n                    <InputGroup.Append>\n                        <Button variant=\"outline-secondary\" onClick={() => onSearch(search)}>Search</Button>\n                    </InputGroup.Append>\n                </InputGroup>\n\n            </Form>\n            {suggestions.length > 0 && (\n                <div className=\"suggestions-list\">\n                    {\n                        suggestions.slice(0, 6).map(s => <Button key={s.id} onClick={() => onSuggestionSelected(s)} variant=\"secondary\">{getShortLabel(s)}</Button>)\n                    }\n                </div>\n            )}\n        </>\n    )\n}\n\nexport default SearchBar;","/home/eugenio/sky/moviedb-em/src/components/ItemList.js",[],"/home/eugenio/sky/moviedb-em/src/components/Credits.js",[],"/home/eugenio/sky/moviedb-em/src/components/utils.js",[],"/home/eugenio/sky/moviedb-em/src/components/MovieDetail.js",["48"],"/home/eugenio/sky/moviedb-em/src/components/PersonDetail.js",[],"/home/eugenio/sky/moviedb-em/src/components/ShowDetail.js",["49"],{"ruleId":"50","severity":1,"message":"51","line":2,"column":62,"nodeType":"52","messageId":"53","endLine":2,"endColumn":73},{"ruleId":"50","severity":1,"message":"54","line":2,"column":28